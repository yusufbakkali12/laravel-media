{
    "name": "bakkali/laravel-media",
    "description": "Simple polymorphic media library for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "media", "polymorphic", "file", "upload"],
    "homepage": "https://github.com/yusufbakkali12/laravel-media",
    "authors": [
        {
            "name": "bakkali youssef",
            "email": "bakkali1998yf@gmail.com",
            "homepage": "https://bakkali.dev",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bakkali\\Media\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bakkali\\Media\\MediaServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}