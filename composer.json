{
    "name": "bakkali/laravel-media",
    "description": "Simple polymorphic media library for Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Bakkali\\Media\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bakkali\\Media\\MediaServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^11.0|^12.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}